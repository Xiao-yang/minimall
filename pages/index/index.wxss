/* 轮播样式 */
swiper{
  width: 750rpx;
  height: 285rpx;
}
image{
  width: 100%;
}
/* 导航框样式 */
.index_cate{
  display: flex;
}
.index_cate navigator{
  padding: 20rpx;
  flex: 1;
}
image{
  width: 100%;
}
/* 楼层样式 */
.floor_tilte{
  padding: 15rpx 0;
}
.floor_tilte image{
  widows: 100%;
}
.floor_list navigator{
  float: left;
  width: 33.33%;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4){
  height: 27.72711207vw;
  border-left: 10rpx solid #fff; 
}
.index_floor .floor_group .floor_list navigator:nth-child(2){
  border-bottom: 10rpx solid #fff; 
}
.index_floor .floor_group .floor_list navigator:nth-child(3){
  border-bottom: 10rpx solid #fff; 
}